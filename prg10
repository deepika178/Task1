package datatypes;

import java.util.Scanner;

public class prg10 {
    public static void main(String[] args)
    {
        Scanner sc = new Scanner(System.in);
        int a = sc.nextInt();
        int n=sc.nextInt();
        int i;
        for(i=1;i<=n;i++)
        {
            int mul=a*i;
            System.out.println(a+"*"+i+"="+mul);
        }


    }
}
